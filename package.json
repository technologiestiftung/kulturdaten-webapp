{
	"name": "kulturdaten-webapp",
	"version": "0.0.0",
	"description": "The GUI of the kulturdaten.berlin app",
	"author": "Technologiestiftung Berlin",
	"license": "MIT",
	"scripts": {
		"postinstall": "next telemetry disable",
		"generate-api": "openapi --input openAPI-specs.yml --output src/api/client --name APIClient --useUnionTypes",
		"run": "NODE_OPTIONS='--inspect' next start",
		"start": "PORT=3001 next dev",
		"turbo": "PORT=3002 next dev --turbo",
		"format": "prettier --write .",
		"lint": "eslint .",
		"typecheck": "tsc --noEmit",
		"build": "next build"
	},
	"dependencies": {
		"@types/lodash": "^4.14.198",
		"autoprefixer": "10.4.15",
		"lodash": "^4.17.21",
		"next": "13.4.7",
		"postcss": "8.4.29",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript-cookie": "^1.0.6"
	},
	"devDependencies": {
		"@types/node": "20.6.0",
		"@types/react": "18.2.21",
		"@types/react-dom": "18.2.7",
		"all-contributors-cli": "^6.26.1",
		"eslint": "8.49.0",
		"eslint-config-next": "13.4.19",
		"openapi-types": "^12.1.3",
		"openapi-typescript-codegen": "^0.25.0",
		"sass": "^1.66.1",
		"tailwindcss": "3.3.3",
		"typescript": "5.2.2"
	},
	"files": [
		"dist"
	]
}
