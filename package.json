{
	"name": "kulturdaten-webapp",
	"version": "0.0.0",
	"description": "The GUI of the kulturdaten.berlin app",
	"author": "Technologiestiftung Berlin",
	"license": "MIT",
	"scripts": {
		"postinstall": "next telemetry disable",
		"generate-api": "openapi --input openAPI-specs.yml --output src/api/client --name APIClient --useUnionTypes",
		"dev": "PORT=3001 next dev",
		"turbo": "PORT=3002 next dev --turbo",
		"prebuild": "npm run generate-api",
		"build": "next build",
		"start": "next start",
		"format": "prettier --write .",
		"lint": "eslint .",
		"typecheck": "tsc --noEmit",
		"test": "vitest"
	},
	"dependencies": {
		"@emotion/react": "11.11.1",
		"@emotion/styled": "11.11.0",
		"@floating-ui/react": "0.25.4",
		"@types/lodash": "^4.14.198",
		"lodash": "^4.17.21",
		"lucide-react": "0.279.0",
		"next": "13.4.7",
		"next-intl": "2.20.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript-cookie": "^1.0.6"
	},
	"devDependencies": {
		"@emotion/babel-plugin": "11.11.0",
		"@types/node": "20.6.0",
		"@types/react": "18.2.21",
		"@types/react-dom": "18.2.7",
		"@typescript-eslint/eslint-plugin": "6.7.0",
		"@typescript-eslint/parser": "6.7.0",
		"all-contributors-cli": "^6.26.1",
		"eslint": "8.49.0",
		"eslint-config-next": "13.4.19",
		"eslint-config-prettier": "9.0.0",
		"eslint-plugin-jsx-a11y": "6.7.1",
		"eslint-plugin-prettier": "5.0.0",
		"eslint-plugin-react": "7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-storybook": "0.6.13",
		"openapi-types": "^12.1.3",
		"openapi-typescript-codegen": "^0.25.0",
		"prettier": "^3.0.3",
		"typescript": "5.2.2",
		"vitest": "0.34.4"
	},
	"files": [
		"dist"
	]
}
