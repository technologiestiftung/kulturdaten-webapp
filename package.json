{
	"name": "kulturdaten-webapp",
	"version": "0.0.0",
	"description": "The GUI of the kulturdaten.berlin app",
	"author": "Technologiestiftung Berlin",
	"license": "MIT",
	"scripts": {
		"postinstall": "next telemetry disable",
		"generate-api": "openapi --input openAPI-specs.yml --output src/api/client --name APIClient --useUnionTypes",
		"dev": "PORT=3001 next dev",
		"prebuild": "npm run generate-api",
		"build": "next build",
		"start": "next start",
		"format": "prettier --write .",
		"lint": "eslint .",
		"typecheck": "tsc --noEmit",
		"test": "vitest"
	},
	"dependencies": {
		"@emotion/react": "11.11.1",
		"@emotion/styled": "11.11.0",
		"@floating-ui/react": "0.26.3",
		"jwt-decode": "4.0.0",
		"lucide-react": "0.294.0",
		"next": "14.0.3",
		"next-intl": "3.2.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-modal": "3.16.1",
		"react-select": "5.8.0",
		"typescript-cookie": "1.0.6"
	},
	"devDependencies": {
		"@emotion/babel-plugin": "11.11.0",
		"@types/node": "20.10.3",
		"@types/react": "18.2.41",
		"@types/react-dom": "18.2.17",
		"@types/react-modal": "3.16.3",
		"@typescript-eslint/eslint-plugin": "6.13.1",
		"@typescript-eslint/parser": "6.13.1",
		"all-contributors-cli": "6.26.1",
		"eslint": "8.55.0",
		"eslint-config-next": "14.0.3",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-jsx-a11y": "6.8.0",
		"eslint-plugin-prettier": "5.0.1",
		"eslint-plugin-react": "7.33.2",
		"eslint-plugin-react-hooks": "4.6.0",
		"eslint-plugin-storybook": "0.6.15",
		"openapi-types": "12.1.3",
		"openapi-typescript-codegen": "0.25.0",
		"prettier": "3.1.0",
		"typescript": "5.3.2",
		"vitest": "0.34.6"
	}
}
