		"classnames": "^2.3.2",
		"eslint": "8.39.0",
		"eslint-config-next": "13.3.1",
		"openapi-types": "^12.1.0",
		"openapi-typescript-codegen": "^0.24.0",
		"sass": "^1.62.0",
		"tailwindcss": "3.3.1",
